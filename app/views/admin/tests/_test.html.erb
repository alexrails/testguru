   <tr class="text-center">
     <td class="align-middle">
       <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %>
       <%= render 'form_inline', test: test == @test ? @test : test %>
     </td>
     <td class="align-middle"><%= test.level %></td>
     <td class="align-middle"><%= questions_count(test) %></td>
     <td class="align-middle"><%= link_to t('navigate.show'), admin_test_path(test), class: 'btn btn-secondary' %></td>
     <td class="align-middle">
       <%= link_to t('navigate.edit'),
         edit_admin_test_path(test),
         data: { test_id: test.id },
         class: 'btn btn-secondary form-inline-link' %>
     </td>
     <td class="align-middle"><%= link_to t('navigate.delete'), admin_test_path(test),
       method: :delete,
       data: { confirm: 'You sure?' }, class: 'btn btn-secondary' %>
     </td>
   </tr>


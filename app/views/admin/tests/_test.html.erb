   <tr class="text-center">
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= questions_count(test) %></td>
      <td><%= link_to t('navigate.show'), admin_test_path(test), class: 'btn btn-secondary' %></td>
      <td><%= link_to t('navigate.edit'), edit_admin_test_path(test), class: 'btn btn-secondary' %></td>
      <td><%= link_to t('navigate.delete'), admin_test_path(test),
        method: :delete,
        data: { confirm: 'You sure?' }, class: 'btn btn-secondary' %>
      </td>
   </tr>


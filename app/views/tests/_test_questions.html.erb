 <% @test.questions.each do |question| %>
    <tr>
      <td><%= question.body %></td>
      <td><%= link_to 'Show', question  %></td>
      <td><%= link_to 'Edit', (edit_question_path question) %><td>
      <td>
          <%= link_to 'Delete', question,
                                method: :delete,
                                data: { confirm: "You sure?" } %>
      </td>
    </tr>
  <% end %>

